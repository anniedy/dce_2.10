#!/bin/bash
dce_images="
dce-kube-controller
dce-etcd
dce-agent
dce
dce-controller
dce-flannel
dce-kube-proxy
dce-calico-node
dce-calico-kube-controllers
dce-calico-cni
dce-kube-dns-sidecar
dce-kube-dns
dce-kube-dnsmasq-nanny
dce-kube-pause
dce-hly"
for image in $dce_images
do
	docker pull {{ upgrade_hub_prefix }}/$image:{{ upgrade_version }}
	docker tag {{ upgrade_hub_prefix }}/$image:{{ upgrade_version }} daocloud.io/daocloud/$image:{{ upgrade_version }}
	docker rmi -f {{ upgrade_hub_prefix }}/$image:{{ upgrade_version }}
done
